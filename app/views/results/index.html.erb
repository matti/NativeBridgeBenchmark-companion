<canvas id="myCanvas" width="320" height="200"></canvas>

<button id="toggleScrolling">Toggle scrolling</button>

<h1>Perform tests</h1>

<button id="loadURL">LoadURL()</button>

<h1>Listing results</h1>

<table border="1">
  <thead>
    <tr>
      <th>Result</th>
      <th>webview to native delta</th>
      <th>native to webview delta</th>

      <th>WebView start</th>
      <th>Native received</th>
      <th>Native started</th>
      <th>WebView received</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @results.each do |result| %>
      <tr>
        <td><%= result.id %></td>
        <td><%= result.delta_webview_to_native %></td>
        <td><%= result.delta_native_to_webview %></td>

        <td><%= result.webview_started_at.to_f %></td>
        <td><%= result.native_received_at.to_f %></td>

        <td><%= result.native_started_at.to_f %></td>
        <td><%= result.webview_received_at.to_f %></td>


        <td><%= link_to 'Edit', edit_test_result_path(@test, result) %></td>
        <td><%= link_to 'Destroy', test_result_path(@test, result), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
